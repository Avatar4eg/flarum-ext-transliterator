  "type": "flarum-extension",
  "keywords": ["url", "language", "latin"],
  "license": "MIT",
  "authors": [
    {
      "name": "Nikolai Morozov",
      "email": "avanmorozov@gmail.com"
    }
  ],
  "require": {
